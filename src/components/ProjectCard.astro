---
import UnifiedCard from './UnifiedCard.astro';
export interface Props {
  title: string;
  description: string;
  source?: string;
  demo?: string;
  stack?: string[];
  image?: string;
  slot?: string;
}
const { title, description, source, demo, stack = [], image, slot } = Astro.props;
---
<UnifiedCard mode="overview" title={title} description={description} source={source} demo={demo} stack={stack} image={image} slot={slot} /> 